            <div>
                <div style="width: 200px; height: 170px" class="border" >
                </div>
            </div>
            <div class="w-100">
                <div class="vertical-justify-between">
                    <div class="pl-3">
                        <h4>@mod.Name</h4>
                        <p class="mb-0" style="color: black;">@mod.Description</p>
                    </div>
                    <div class="d-flex justify-content-end">
                        <button id="@mod.Id" onclick="AddModel(this)" class="btn btn-sm btn-success">PÅ™idat</button>
                    </div>
                </div>
            </div>
        </li>
    }
