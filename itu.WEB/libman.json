{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "bootstrap@4.5.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap.min.css.map",
        "dist/js/bootstrap.bundle.min.js",
        "dist/js/bootstrap.bundle.min.js.map"
      ]
    },
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.2",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "jquery.validate.min.js",
        "localization/messages_cs.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@5.14.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff",
        "webfonts/fa-solid-900.woff2"
      ]
    },
    {
      "library": "bootstrap-fileinput@5.2.5",
      "destination": "wwwroot/lib/bootstrap-fileinput/",
      "files": [
        "css/fileinput.min.css",
        "js/fileinput.min.js",
        "js/locales/cs.min.js",
        "img/loading.gif",
        "themes/explorer-fas/theme.min.js",
        "themes/explorer-fas/theme.min.css",
        "themes/explorer/theme.min.css",
        "themes/explorer/theme.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/microsoft/signalr/"
    }
  ]
}
